
$(document).ready(function(){
  
  /*Willy Weather Fix*/
  //$("#weather iframe").remove();
  
  
  /* Product Page Slider */
	if(jQuery('#product_slider').length) {
		var pslider = jQuery('#product_slider .slides').bxSlider({
			pager: false,
			controls: false,
			speed: 500,
			adaptiveHeight: true,
            //useCSS: false,
            infiniteLoop: false, // do not turn this on, it causes a bug which shows the last slide first in Chrome
			onSliderLoad: function(){
				$('#product_slider .loader').remove();
				$('#product_slider').css('height', 'auto');
              	$('#product_thumb_slider li').first().addClass('active');
			}, 
            onSlideAfter: function($slideElement, oldIndex, newIndex) {
        		$('#product_thumb_slider li').removeClass('active');
          		$('#product_thumb_slider li[data-slide-index='+newIndex+']').addClass('active');
        	}
		});	
	};	

	$('#product_thumb_slider li').click(function(){
      	jQuery('#product_thumb_slider li').removeClass('active');
      	jQuery(this).addClass('active');
		var slideindex = $(this).attr('data-slide-index');
		pslider.goToSlide(slideindex);
	});
  
    $('#product_thumb_slider .slides').bxSlider({
      minSlides: 2,
      maxSlides: 4,
      slideWidth: 111,
      slideMargin: 5,
      adaptiveHeight: true,
      pager: false,
      //useCSS: false,
      infiniteLoop: false
    });
  
  $('.mobile_menu > li > a').click(function(){
    if ($(this).hasClass('no-drop')) {
    }else {
      $(this).addClass('active');
      $(this).next().slideToggle();
      return false;
    }
  });
  
  /* Prevent phone keyboard from displaying over datepicker */
  $('#booking-start').prop('readonly', true);
  $('#booking-start-2').prop('readonly', true);
  $('#booking-start-3').prop('readonly', true);
  $('#booking-start-4').prop('readonly', true);
  
});



$(function() {
  
  $('#sidebar_slider').flexslider({
    touch: false, 
    {% if settings.video_embed != blank %}
      pauseOnHover: true,
      {% if settings.video_position == '1' %}
        slideshow: false,
      {% endif %}
    {% endif %}
    controlNav: false,
    directionNav: false,
    animation: "{{ settings.slideshow_animation }}", 
    {% if settings.slideshow_animation == 'slide' %}
      useCSS: false,
    {% endif %}    
    slideshowSpeed: 10000,
  });
  
  $('#sidebar_slider2').flexslider({
    touch: false, 
    {% if settings.video_embed != blank %}
      pauseOnHover: true,
      {% if settings.video_position == '1' %}
        slideshow: false,
      {% endif %}
    {% endif %}
    controlNav: false,
    directionNav: false,
    animation: "{{ settings.slideshow_animation }}", 
    {% if settings.slideshow_animation == 'slide' %}
      useCSS: false,
    {% endif %}    
    slideshowSpeed: 10000,
  });    
  
  $('#homepage_slider').flexslider({
    touch: false, 
    {% if settings.video_embed != blank %}
      pauseOnHover: true,
      video: true,
      {% if settings.video_position == '1' %}
        slideshow: false,
      {% endif %}
    {% endif %}
    controlNav: true, 
    animation: "{{ settings.slideshow_animation }}", 
    {% if settings.slideshow_animation == 'slide' %}
      useCSS: false,
    {% endif %}    
    slideshowSpeed: {% if settings.slideshow_speed != blank %}{{ settings.slideshow_speed }}{% else %}10{% endif %}*1000 
  });

  if($('#homepage_slider img').length == 1) { $('#homepage_slider .flex-direction-nav').hide() }

  $('.slider').flexslider({touch: false, controlNav: false, animation: "{{ settings.slideshow_animation }}" });
  
  $('.slider-no-nav').flexslider({touch: false, controlNav: false, directionNav: false, animation: "{{ settings.slideshow_animation }}" });

  $('.slider-no-nav-quick').flexslider({touch: false, controlNav: false, directionNav: false, animation: "{{ settings.slideshow_animation }}", slideshowSpeed: 4000 });

  $('.navicon_button').click(function() {
    $('.toggle_menu').toggle();
  });
  



  

  $('.lightbox').fancybox();
  
  if ($(window).width() >= 959) {
    $('.thumbnail').hover(function(){ $('.quick_shop', this).show(); }, function(){ $('.quick_shop', this).hide(); })
    $(".quick_shop").fancybox({
      width: '840px',
      height: 'auto',
      autoSize: false,
      padding: [0, 20, 0, 20]
    });
  } 

  if ($(window).width() >= 768) {
    $(".fancybox").fancybox({padding: 0, wrapCSS: 'gallery'});
  }

  {% if settings.product_slideshow_animation == 'zoom' %}
    {% if settings.product_images_position == 'right' %}
      $.fn.CloudZoom.defaults.zoomPosition = 13;
    {% endif %}
    CloudZoom.quickStart();
  {% endif %}

  $('#tag_filter').change(function() { window.location = $('#tag_filter option:selected').val(); });  
  $('.update_subtotal').click(function() { $("#cart_form").submit(); return false; });
  $("#cart_form input[type='number']").change(function() {$("#cart_form").submit(); });
  
  Shopify.queryParams = {};
  if (location.search.length) {
    for (var aKeyValue, i = 0, aCouples = location.search.substr(1).split('&'); i < aCouples.length; i++) {
      aKeyValue = aCouples[i].split('=');
      if (aKeyValue.length > 1) {
        Shopify.queryParams[decodeURIComponent(aKeyValue[0])] = decodeURIComponent(aKeyValue[1]);
      }
    }
  }

  var tabs = $('ul.tabs');
  tabs.each(function(i) {
    var tab = $(this).find('> li > a');
    tab.click(function(e) {
      var contentLocation = $(this).attr('href');
      if(contentLocation.charAt(0)=="#") {
        e.preventDefault();
        tab.removeClass('active');
        $(this).addClass('active');
        $(this).parents('ul.tabs').next().find(contentLocation).show().css({'display': 'block'}).addClass('active').siblings().hide().removeClass('active');
      }
    });
  });

  $('.toggle').click(function() {
    if ($(window).width() <= 767) { 
      var $toggle = $(this);
      if($toggle.next('ul').is(':visible')) {
        $toggle.next('ul').hide();
        $toggle.children('span').html('+');
      } else {
        $toggle.next('ul').show();
        $toggle.children('span').html('-');
      }
    }
  });

  var $contact_form = $('.newsletter .contact-form');
  $contact_form.each(function() {
    var $cf = $(this);
    $cf.on("submit", function (e) {
      {% if settings.newsletter_system != 'mailchimp' %}
        $.ajax({
            type: $cf.attr('method'),
            url: $cf.attr('action'),
            data: $cf.serialize(),
            success: function (data) {
              $cf.prev('.message').html({{ 'general.newsletter_form.success_text' | t | json }});
            }
        });
        e.preventDefault();
      {% endif %}
      setTimeout($.fancybox.close, 2000);
    });
  });

  {% raw %}
    $(function() {
      var $target = $("#instafeed");
      $target.instagramLite({
        username: $target.data('username'),
        clientID: $target.data('client-id'),
        urls: true,
        limit: $target.data('count'),
        error: function(errorCode, errorMessage) {
          if(errorCode && errorMessage) {
            alert(errorCode +': '+ errorMessage);
          } 
        }
        });
      });
  {% endraw %}  

  selectCallback = function(variant, selector) {
    
    // BEGIN SWATCHES
    if (variant) {
      var form = jQuery('#' + selector.domIdPrefix).closest('form');
      for (var i=0,length=variant.options.length; i<length; i++) {
        var radioButton = form.find('.swatch[data-option-index="' + i + '"] :radio[value="' + variant.options[i] +'"]');
        if (radioButton.size()) {
          //radioButton.get(0).checked = true;
        }
      }
    }
    // END SWATCHES
    var $product = $('#product-' + selector.product.id);
    var $notify_form = $('#notify-form-' + selector.product.id);
    var items_left_text = "";

    if (variant && variant.featured_image && $product.is(":visible")) {
      var $slider = $('#product_slider', $product);
      var original_image = $(".flex-active-slide img", $product), new_image = variant.featured_image;
      Shopify.Image.switchImage(new_image, original_image[0], function (new_image_src, original_image, element) {
        if ($slider.length) {
          $slider.flexslider($('[data-image-id="' + variant.featured_image.id + '"]').data('index'));
        } else {
          $(element, $product).attr('src', new_image_src);
        }
      });
    }

    if (variant && variant.available == true) {
      if(variant.price < variant.compare_at_price){
        $('.was_price', $product).html(Shopify.formatMoney(variant.compare_at_price, $('form.product_form', $product).data('money-format')))        
      } else {
        $('.was_price', $product).text('')
      } 

      {% if settings.display_inventory_left %}
        if (variant.inventory_management == "shopify" && variant.inventory_quantity > 0) {
          if (variant.inventory_quantity == 1) {
            items_left_text = {{ 'products.product.items_left_count.one' | t | json }};
          } else {
            items_left_text = {{ 'products.product.items_left_count.other' | t | json }};
          }
          $('.items_left', $product).html("<p><em>" + variant.inventory_quantity + " " + items_left_text +"</em></p>");
          $('.quantity', $product).attr('max', variant.inventory_quantity);
        } else {
          $('.items_left', $product).text('');
          $('.quantity', $product).removeAttr('max');
        }
      {% endif %}

      $('.sold_out', $product).text('');
      $('.current_price', $product).html(Shopify.formatMoney(variant.price, $('form.product_form', $product).data('money-format')));
      $('.add_to_cart', $product).removeClass('disabled').removeAttr('disabled').val('{{ 'products.product.add_to_cart' | t }}');
      $notify_form.hide();
    } else {
      var message = variant ? "{{ 'products.product.sold_out' | t }}" : "{{ 'products.product.unavailable' | t }}";    
      $('.was_price', $product).text('');
      $('.current_price', $product).text('');
      $('.items_left', $product).text('');
      $('.quantity', $product).removeAttr('max');      
      $('.sold_out', $product).text(message);
      $('.add_to_cart', $product).addClass('disabled').attr('disabled', 'disabled').val(message); 
      $notify_form.fadeIn();
    }

    {% if settings.show_multiple_currencies %}
      if($('#currencies option:selected').val()) {
        Currency.convertAll($('form.product_form', $product).data('shop-currency'), $('#currencies option:selected').val()); 
      }
    {% endif %}
  };          
});

